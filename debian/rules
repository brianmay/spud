#!/usr/bin/make -f
# -*- makefile -*-

PREFIX = debian/spud

export HOME=${CURDIR}/tmp
export PYBUILD_NAME=spud

%:
	dh $@ --with python-virtualenv

override_dh_virtualenv:
	tsc spud/static/js/*.ts
	dh_virtualenv --python python3

.PHONY: override_dh_clean
override_dh_clean:
	dh_clean
	rm -rf tmp spud/static/js/*.js
